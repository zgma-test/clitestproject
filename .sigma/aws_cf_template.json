{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "This template creates the stack for project CLITestProject. Generated by Sigma",
  "Parameters": {
    "lambdaCodeBucketParameter": {
      "Type": "String",
      "Description": "Name of the S3 bucket containing the Lambda deployment package"
    },
    "lambdaCodeKeyParameter": {
      "Type": "String",
      "Description": "S3 key of the Lambda deployment package"
    },
    "envVarMySecretVarCLITestNodeLambdaParameter": {
      "Type": "String",
      "Description": "Value of the environment variable: MySecretVar, for Lambda: CLITestNodeLambda"
    }
  },
  "Outputs": {
    "httpApigMyEventAPIRoutePOSTvalidate": {
      "Description": "POST endpoint for route /validate on API MyEventAPI",
      "Value": {
        "Fn::Join": [
          "",
          [
            "https://",
            {
              "Ref": "httpApigMyEventAPI"
            },
            ".execute-api.",
            "us-east-1",
            ".amazonaws.com",
            "/test",
            "/validate"
          ]
        ]
      }
    },
    "httpApigMyEventAPIRouteGETtime": {
      "Description": "GET endpoint for route /time on API MyEventAPI",
      "Value": {
        "Fn::Join": [
          "",
          [
            "https://",
            {
              "Ref": "httpApigMyEventAPI"
            },
            ".execute-api.",
            "us-east-1",
            ".amazonaws.com",
            "/test",
            "/time"
          ]
        ]
      }
    },
    "snsusEast1MyNotificationTopic": {
      "Description": "ARN of SNS topic MyNotificationTopic",
      "Value": {
        "Ref": "snsusEast1MyNotificationTopic"
      }
    },
    "CLITestNodeLambda": {
      "Description": "ARN of Lambda function CLITestNodeLambda",
      "Value": {
        "Fn::GetAtt": [
          "CLITestNodeLambda",
          "Arn"
        ]
      }
    },
    "CLITestPyLambda": {
      "Description": "ARN of Lambda function CLITestPyLambda",
      "Value": {
        "Fn::GetAtt": [
          "CLITestPyLambda",
          "Arn"
        ]
      }
    }
  },
  "Resources": {
    "httpApigMyEventAPI": {
      "Type": "AWS::ApiGatewayV2::Api",
      "Properties": {
        "Description": "An HTTP API generated by Sigma",
        "Name": "MyEventAPI",
        "ProtocolType": "HTTP"
      }
    },
    "httpApigMyEventAPIStage": {
      "Type": "AWS::ApiGatewayV2::Stage",
      "Properties": {
        "ApiId": {
          "Ref": "httpApigMyEventAPI"
        },
        "Description": "Stage of API",
        "StageName": "test",
        "AutoDeploy": true
      }
    },
    "snsusEast1MyNotificationTopic": {
      "Type": "AWS::SNS::Topic",
      "Properties": {
        "DisplayName": "Notifications",
        "Subscription": [],
        "TopicName": "MyNotificationTopic"
      }
    },
    "CLITestNodeLambda": {
      "Type": "AWS::Lambda::Function",
      "Properties": {
        "FunctionName": "CLITestNodeLambda",
        "Description": "Invokes functions defined in CLITestNodeLambda.js in project CLITestProject. Generated by Sigma.",
        "Code": {
          "S3Bucket": {
            "Ref": "lambdaCodeBucketParameter"
          },
          "S3Key": {
            "Ref": "lambdaCodeKeyParameter"
          }
        },
        "MemorySize": 128,
        "Handler": "CLITestNodeLambda.handler",
        "Timeout": 60,
        "Runtime": "nodejs10.x",
        "Layers": [],
        "TracingConfig": {
          "Mode": "PassThrough"
        },
        "Role": {
          "Fn::GetAtt": [
            "CLITestNodeLambdaExecutionRole",
            "Arn"
          ]
        },
        "Environment": {
          "Variables": {
            "MyVar": "Hello Node",
            "MySecretVar": {
              "Ref": "envVarMySecretVarCLITestNodeLambdaParameter"
            },
            "SIGMA_AWS_ACC_ID": {
              "Ref": "AWS::AccountId"
            }
          }
        }
      }
    },
    "CLITestNodeLambdaExecutionRole": {
      "Type": "AWS::IAM::Role",
      "Properties": {
        "ManagedPolicyArns": [
          "arn:aws:iam::aws:policy/service-role/AWSLambdaBasicExecutionRole"
        ],
        "AssumeRolePolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Action": [
                "sts:AssumeRole"
              ],
              "Effect": "Allow",
              "Principal": {
                "Service": [
                  "lambda.amazonaws.com"
                ]
              }
            }
          ]
        },
        "Policies": [
          {
            "PolicyName": "CLITestNodeLambdaPolicy",
            "PolicyDocument": {
              "Version": "2012-10-17",
              "Statement": [
                {
                  "Effect": "Allow",
                  "Action": [
                    "sns:Publish"
                  ],
                  "Resource": {
                    "Ref": "snsusEast1MyNotificationTopic"
                  }
                }
              ]
            }
          }
        ]
      }
    },
    "httpApigMyEventAPIIntegrationCLITestNodeLambda": {
      "Type": "AWS::ApiGatewayV2::Integration",
      "Properties": {
        "ApiId": {
          "Ref": "httpApigMyEventAPI"
        },
        "Description": "Integration of function: CLITestNodeLambda to HTTP API: MyEventAPI",
        "IntegrationType": "AWS_PROXY",
        "PayloadFormatVersion": "2.0",
        "IntegrationUri": {
          "Fn::GetAtt": [
            "CLITestNodeLambda",
            "Arn"
          ]
        }
      }
    },
    "httpApigMyEventAPIRoutePOSTvalidate": {
      "Type": "AWS::ApiGatewayV2::Route",
      "DependsOn": [
        "httpApigMyEventAPIIntegrationCLITestNodeLambda"
      ],
      "Properties": {
        "ApiId": {
          "Ref": "httpApigMyEventAPI"
        },
        "RouteKey": "POST /validate",
        "AuthorizationType": "NONE",
        "Target": {
          "Fn::Join": [
            "/",
            [
              "integrations",
              {
                "Ref": "httpApigMyEventAPIIntegrationCLITestNodeLambda"
              }
            ]
          ]
        }
      }
    },
    "httpApigMyEventAPIRoutePOSTvalidatePermission": {
      "Type": "AWS::Lambda::Permission",
      "Properties": {
        "Action": "lambda:InvokeFunction",
        "FunctionName": {
          "Ref": "CLITestNodeLambda"
        },
        "SourceArn": {
          "Fn::Sub": [
            "arn:aws:execute-api:${AWS::Region}:${AWS::AccountId}:${__ApiId__}/*/${__Method__}${__Path__}",
            {
              "__Method__": "POST",
              "__Path__": "/validate",
              "__ApiId__": {
                "Ref": "httpApigMyEventAPI"
              }
            }
          ]
        },
        "Principal": "apigateway.amazonaws.com"
      }
    },
    "CLITestPyLambda": {
      "Type": "AWS::Lambda::Function",
      "Properties": {
        "FunctionName": "CLITestPyLambda",
        "Description": "Invokes functions defined in CLITestPyLambda.py in project CLITestProject. Generated by Sigma.",
        "Code": {
          "S3Bucket": {
            "Ref": "lambdaCodeBucketParameter"
          },
          "S3Key": {
            "Ref": "lambdaCodeKeyParameter"
          }
        },
        "MemorySize": 128,
        "Handler": "CLITestPyLambda.handler",
        "Timeout": 60,
        "Runtime": "python3.8",
        "Layers": [],
        "TracingConfig": {
          "Mode": "PassThrough"
        },
        "Role": {
          "Fn::GetAtt": [
            "CLITestPyLambdaExecutionRole",
            "Arn"
          ]
        },
        "Environment": {
          "Variables": {
            "MyVar": "Hello Python",
            "SIGMA_AWS_ACC_ID": {
              "Ref": "AWS::AccountId"
            }
          }
        }
      }
    },
    "CLITestPyLambdaExecutionRole": {
      "Type": "AWS::IAM::Role",
      "Properties": {
        "ManagedPolicyArns": [
          "arn:aws:iam::aws:policy/service-role/AWSLambdaBasicExecutionRole"
        ],
        "AssumeRolePolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Action": [
                "sts:AssumeRole"
              ],
              "Effect": "Allow",
              "Principal": {
                "Service": [
                  "lambda.amazonaws.com"
                ]
              }
            }
          ]
        }
      }
    },
    "httpApigMyEventAPIIntegrationCLITestPyLambda": {
      "Type": "AWS::ApiGatewayV2::Integration",
      "Properties": {
        "ApiId": {
          "Ref": "httpApigMyEventAPI"
        },
        "Description": "Integration of function: CLITestPyLambda to HTTP API: MyEventAPI",
        "IntegrationType": "AWS_PROXY",
        "PayloadFormatVersion": "2.0",
        "IntegrationUri": {
          "Fn::GetAtt": [
            "CLITestPyLambda",
            "Arn"
          ]
        }
      }
    },
    "httpApigMyEventAPIRouteGETtime": {
      "Type": "AWS::ApiGatewayV2::Route",
      "DependsOn": [
        "httpApigMyEventAPIIntegrationCLITestPyLambda"
      ],
      "Properties": {
        "ApiId": {
          "Ref": "httpApigMyEventAPI"
        },
        "RouteKey": "GET /time",
        "AuthorizationType": "NONE",
        "Target": {
          "Fn::Join": [
            "/",
            [
              "integrations",
              {
                "Ref": "httpApigMyEventAPIIntegrationCLITestPyLambda"
              }
            ]
          ]
        }
      }
    },
    "httpApigMyEventAPIRouteGETtimePermission": {
      "Type": "AWS::Lambda::Permission",
      "Properties": {
        "Action": "lambda:InvokeFunction",
        "FunctionName": {
          "Ref": "CLITestPyLambda"
        },
        "SourceArn": {
          "Fn::Sub": [
            "arn:aws:execute-api:${AWS::Region}:${AWS::AccountId}:${__ApiId__}/*/${__Method__}${__Path__}",
            {
              "__Method__": "GET",
              "__Path__": "/time",
              "__ApiId__": {
                "Ref": "httpApigMyEventAPI"
              }
            }
          ]
        },
        "Principal": "apigateway.amazonaws.com"
      }
    }
  }
}